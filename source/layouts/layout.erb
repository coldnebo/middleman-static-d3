<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "static d3 example" %></title>
    
    <%= stylesheet_embed "normalize" %>
    <%= javascript_embed "d3.min" %>
    <script type="text/javascript">var data_embed = {};</script>
  </head>
  
  <body>
    <%= yield %>
  </body>
</html>

